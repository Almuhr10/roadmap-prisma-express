generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  username  String   @unique
  password  String
  loan Loan[]
}

model Books {
  id      String   @id @default(uuid())
  name    String
  genre   String
  loan Loan[]
}
model Loan {
  user_ID      String @id
  book_ID String
  userID User @relation(fields: [user_ID] ,references: [id])
  bookId Books @relation(fields: [book_ID], references: [id])
}

